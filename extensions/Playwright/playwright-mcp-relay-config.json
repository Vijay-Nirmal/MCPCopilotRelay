  "toolSetName": "playwright",
  "toolSetDescription": "Playwright tool set provides browser automation capabilities, enabling Copilot to interact with web pages through structured accessibility snapshots, bypassing the need for screenshots or visually-tuned models",
  "enableToolSets": false,
  "extensionInfo": {
    "name": "playwright-mcp-relay",
    "displayName": "Playwright MCP Relay",
    "description": "Playwright MCP Relay provides browser automation capabilities using Playwright, enabling Copilot to interact with web pages as VSCode Language Model Tools, which means this extension works even if MCP was disabled in Github Copilot and VSCode",
    "version": "1.0.0",
    "publisher": "VijayNirmal",
    "author": "Vijay Nirmal",
    "license": "MIT",
    "repository": "https://github.com/Vijay-Nirmal/MCPCopilotRelay",
    "icon": "playwright-logo.png",
    "iconFileName": "playwright-logo.png",
    "iconFileData": "data:image/png;base64,",
    "settings": [
      {
        "key": "additionalMCPServerArguments",
        "type": "string",
        "description": "Additional MCP Server arguments that need to be used when connecting to the MCP Server. You can find the argument details [here](https://github.com/microsoft/playwright-mcp?tab=readme-ov-file#configuration)",
        "default": "",
        "mcpMapping": {
          "target": "dynamic-arg",
          "key": "",
          "required": false
        },
        "secret": false
      }
    ],
    "categories": [
      "AI",
      "Chat"
    ],
    "keywords": [
      "mcp",
      "copilot",
      "automation",
      "playwright"
    ],
    "homepage": "https://github.com/Vijay-Nirmal/MCPCopilotRelay",
    "bugs": "https://github.com/Vijay-Nirmal/MCPCopilotRelay/issues",
    "qna": "marketplace",
